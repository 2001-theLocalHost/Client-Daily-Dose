{
  "expo": {
    "name": "Daily Dose",
    "slug": "Daily-Dose",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "description": "Daily Dose is a nutrition tracking mobile app that uses image recognition to process food images and analyze nutritious intakes.",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": { "sourceExts": ["js", "jsx", "svg", "ts", "tsx", "ttf"] }
  }
}


